    "@types/cli-spinner": "^0.2.0",
    "@types/commander": "^2.12.2",
    "@types/del": "^4.0.0",
    "@types/express": "^4.17.7",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^9.0.0",
    "@types/mongodb": "^3.5.26",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.13.0",
    "chai": "^4.3.4",
    "husky": "^7.0.4",
    "nock": "^13.1.3",
