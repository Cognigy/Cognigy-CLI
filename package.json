{
  "name": "@cognigy/cognigy-cli",
  "version": "2.2.0",
  "description": "Cognigy Command Line Interface",
  "main": "./build/cognigy.js",
  "scripts": {
    "build": "npm run build:production && npm run test",
    "build:debug": "rimraf ./build && tsc && npm run execution-permission",
    "build:production": "rimraf ./build && npm run tsc:production && npm run execution-permission",
    "execution-permission": "chmod +x ./build/cognigy.js",
    "test": "rimraf ./coverage && nyc mocha --require chai-autoload-plugins --recursive ./build/spec --exit --timeout 120000 --color --check-leaks  && rimraf ./.nyc_output",
    "test:tsmocha": "ts-mocha -p tsconfig.json --experimental-worker --recursive './src/spec/**/*.ts' --timeout 120000 --preserve-symlinks",
    "install-from-npm": "npm --registry=https://registry.npmjs.com/ i",
    "tsc:production": "tsc -p tsconfig.production.json",
    "cp-config": "cp ./config-dist.json ./config.json",
    "prepare": "husky install"
  },
  "author": "Cognigy GmbH",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "18.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@commitlint/prompt": "20.4.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@types/cli-progress": "3.11.6",
    "@types/cli-spinner": "0.2.3",
    "@types/express": "5.0.6",
    "@types/inquirer": "6.5.0",
    "@types/mocha": "10.0.10",
    "@types/mongodb": "4.0.7",
    "@types/mongoose": "5.11.97",
    "@types/node": "24.5.2",
    "@types/sinon": "21.0.0",
    "chai": "4.3.4",
    "chai-autoload-plugins": "0.1.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "mocha": "11.7.5",
    "nock": "14.0.10",
    "nyc": "17.1.0",
    "prettier": "3.8.1",
    "sinon": "21.0.1",
    "ts-mocha": "11.1.0",
    "tslint": "6.1.3",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "-": "0.0.1",
    "@cognigy/rest-api-client": "2025.23.0",
    "@google-cloud/translate": "9.3.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@langchain/community": "1.1.14",
    "@langchain/core": "1.1.24",
    "axios": "1.13.5",
    "chalk": "5.6.2",
    "cheerio": "1.2.0",
    "cli-progress": "3.12.0",
    "cli-spinner": "0.2.10",
    "commander": "14.0.3",
    "csv-parser": "3.2.0",
    "csv-writer": "1.6.0",
    "d3-dsv": "2.0.0",
    "diff": "8.0.3",
    "epub2": "3.0.2",
    "express": "5.2.1",
    "form-data": "4.0.4",
    "fs-extra": "11.3.3",
    "gpt-3-encoder": "1.1.4",
    "html-to-text": "9.0.5",
    "inquirer": "13.2.2",
    "js-yaml": "4.1.1",
    "json-diff": "1.0.6",
    "jsonpointer": "5.0.1",
    "mammoth": "1.11.0",
    "os": "0.1.2",
    "pdf-parse": "1.1.1",
    "pdfjs": "2.5.4",
    "playwright": "1.58.2",
    "rimraf": "6.1.3",
    "srt-parser-2": "1.1.3",
    "uuid": "13.0.0"
  },
  "resolutions": {
    "@langchain/core": "1.1.24"
  },
  "bin": {
    "cognigy": "./build/cognigy.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=9.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cognigy/Cognigy-CLI"
  },
  "overrides": {
    "form-data": "4.0.4",
    "cross-spawn": "7.0.5",
    "srt-parser-2": "1.1.3",
    "axios": "1.13.5",
    "tmp": "0.2.5",
    "brace-expansion": "1.1.12",
    "google-gax": {
      "form-data": "2.5.4"
    },
    "google-auth-library": {
      "jws": "4.0.1"
    },
    "jsonwebtoken": {
      "jws": "3.2.3"
    },
    "undici": "7.18.2",
    "@actions/http-client": {
      "undici": "6.23.0"
    },
    "@google-cloud/translate": {
      "minimatch": "10.2.2"
    },
    "glob@^10.0.0": "10.5.0",
    "glob@^11.0.0": "11.1.0",
    "lodash": "4.17.23"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
  }
}
