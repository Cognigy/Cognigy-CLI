    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch CognigyCLI clone",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
			"runtimeArgs": [
				"--nolazy",
				"-r",
				"ts-node/register/transpile-only",
				"--preserve-symlinks"
			],
            "args":["./src/cognigy.ts","clone","--config", "./config.json", "--forceYes"],
            "console": "integratedTerminal"
        }
    ]
}